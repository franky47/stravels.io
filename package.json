{
  "name": "stravels.io",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mapbox/polyline": "^0.2.0",
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "@turf/bbox": "^5.1.5",
    "@turf/helpers": "^6.0.1",
    "apollo-boost": "^0.1.7",
    "apollo-link-context": "^1.0.5",
    "autoprefixer": "8.0.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.2.2",
    "babel-loader": "7.1.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.1",
    "css-loader": "0.28.9",
    "dotenv": "5.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "4.18.2",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.45.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.6.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.7",
    "fs-extra": "5.0.0",
    "graphql": "^0.13.1",
    "graphql-tag": "^2.8.0",
    "html-webpack-plugin": "2.30.1",
    "jest": "24.0.0",
    "mapbox-gl": "^0.44.1",
    "nanoid": "^1.0.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.0",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-media": "^1.8.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-persist": "^5.9.1",
    "style-loader": "0.20.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "analyse": "source-map-explorer build/static/js/main.*",
    "fix:surge-prod": "cp ./build/index.html ./build/200.html",
    "deploy:staging": "surge -d https://pwa.stravels.io -p ./build",
    "deploy:prod": "surge -d https://stravels.io -p ./build",
    "release:staging": "yarn build && yarn fix:surge-prod && yarn deploy:staging",
    "release": "yarn build && yarn fix:surge-prod && yarn deploy"
  },
  "devDependencies": {
    "flow-bin": "^0.70.0",
    "lighthouse": "^2.9.4",
    "react-hot-loader": "^4.0.0",
    "source-map-explorer": "^1.5.0",
    "surge": "^0.19.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "flow"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
